[targets]
node1    ansible_ssh_host=node1.my.aws
node2    ansible_ssh_host=node2.my.aws
st2      ansible_ssh_host=st2.my.aws

[targets:vars]
ansible_user=root
# Use ansible-play `--key-file` to supply your key
ansible_ssh_private_key_file=~/.ssh/your_aws_private_key.pem

[docker_engine]
node1
st2

[docker_swarm_manager]
st2

[docker_swarm_worker]
node1

[docker_registry]
st2
